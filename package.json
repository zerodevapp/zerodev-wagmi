{
  "name": "@zerodev/wagmi",
  "version": "4.1.8",
  "files": [
    "dist",
    "rainbowkit",
    "connectkit",
    "web3modal"
  ],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./rainbowkit": {
      "types": "./dist/types/integrations/rainbowkit/index.d.ts",
      "import": "./dist/esm/integrations/rainbowkit/index.js",
      "default": "./dist/cjs/integrations/rainbowkit/index.js"
    },
    "./connectkit": {
      "types": "./dist/types/integrations/connectkit/index.d.ts",
      "import": "./dist/esm/integrations/connectkit/index.js",
      "default": "./dist/cjs/integrations/connectkit/index.js"
    },
    "./web3modal": {
      "types": "./dist/types/integrations/web3modal/index.d.ts",
      "import": "./dist/esm/integrations/web3modal/index.js",
      "default": "./dist/cjs/integrations/web3modal/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.json --module es2015 --outDir ./dist/esm --removeComments && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "build:types": "tsc --project tsconfig.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types"
  },
  "peerDependencies": {
    "@wagmi/core": ">=1.3.x",
    "react": ">=17",
    "react-dom": ">=17",
    "viem": ">=1.1.7"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zerodev/sdk": "4.0.20",
    "@zerodev/web3auth": "4.1.6",
    "ethers": "5.7.2",
    "wagmi": "^1.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "description": ""
}
